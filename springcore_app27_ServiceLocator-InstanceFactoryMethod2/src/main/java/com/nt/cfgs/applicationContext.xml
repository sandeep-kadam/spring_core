<?xml version="1.0" encoding="UTF-8"?>
<beans     xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="serviceLocator"  class="com.nt.locator.ICCScoreCompServiceLocator" />
  
  <bean id="extServiceLocator"   factory-bean="serviceLocator" factory-method="getService">
     <constructor-arg  value="icc"/>
  </bean>

  
  <bean id="cbService"  class="com.nt.service.CrickBuzzFindScoreServiceImpl">
       <property name="extService"  ref="extServiceLocator"/>
  </bean>
   
</beans>